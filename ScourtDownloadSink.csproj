<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Product>ScourtDownloadSink</Product>
    <Version>1.0.0</Version>
    <Authors>Perdition</Authors>
    <PackageProjectUrl>https://github.com/Perdition-117/ScourtDownloadSink</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Perdition-117/ScourtDownloadSink.git</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Extensions" Version="2.2.0" />
  </ItemGroup>

</Project>
